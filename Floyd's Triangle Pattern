// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter rows - ";
    cin>>n;
    int count = 0;
    //For growing phase
    for(int row=0;row<n;row++){
        for(int col=0;col<row+1;col++){
          count++;
          cout<<count<<" ";
        }
        cout<<endl;
    }
    
    

    return 0;
}
